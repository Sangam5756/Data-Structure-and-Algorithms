import java.util.*;

public class QueueUsingJavacollectionframework{

    public static void main(String args[]){

        Queue<Integer> q = new LinkedList<>();  //ArrayDeque
        q.add(1);
        q.add(2);
        q.isEmpty();
        q.remove();


        while(!q.isEmpty()){
            System.out.println(q.peek());
            q.remove();
        }

    }
}
